{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server.js"
      },
      {
        "src": "/static/(.*)",
        "dest": "/",
        "methods": ["GET", "POST", "DELETE", "PUT"]
      }
    ]
  }